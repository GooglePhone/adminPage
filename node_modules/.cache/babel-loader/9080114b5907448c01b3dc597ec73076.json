{"ast":null,"code":"import { useStore } from 'vuex';\nimport { computed, isProxy, toRaw, nextTick } from 'vue';\nexport default {\n  __name: 'CurrencyOrder',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const store = useStore();\n    const currentOrder = computed(() => {\n      if (isProxy(store.state.currencyOrder)) {\n        //this If() block is not really necessary\n        return toRaw(store.state.currencyOrder);\n      }\n\n      return null;\n    });\n\n    function changeStatus(item) {\n      nextTick(() => {\n        item.status = item.status == 0 ? 1 : 0;\n        console.log(item.status);\n      });\n    }\n\n    const __returned__ = {\n      store,\n      currentOrder,\n      changeStatus,\n      useStore,\n      computed,\n      isProxy,\n      toRaw,\n      nextTick\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAwEA,SAASA,QAAT,QAAyB,MAAzB;AACA,SAASC,QAAT,EAAmBC,OAAnB,EAA4BC,KAA5B,EAAmCC,QAAnC,QAAmD,KAAnD;;;;;;;;IACA,MAAMC,KAAK,GAAGL,QAAQ,EAAtB;IACA,MAAMM,YAAY,GAAGL,QAAQ,CAAC,MAAM;MAClC,IAAIC,OAAO,CAACG,KAAK,CAACE,KAAN,CAAYC,aAAb,CAAX,EAAwC;QACtC;QACA,OAAOL,KAAK,CAACE,KAAK,CAACE,KAAN,CAAYC,aAAb,CAAZ;MACD;;MACD,OAAO,IAAP;IACD,CAN4B,CAA7B;;IAOA,SAASC,YAAT,CAAsBC,IAAtB,EAA4B;MAC1BN,QAAQ,CAAC,MAAM;QACbM,IAAI,CAACC,MAAL,GAAcD,IAAI,CAACC,MAAL,IAAe,CAAf,GAAmB,CAAnB,GAAuB,CAArC;QACAC,OAAO,CAACC,GAAR,CAAYH,IAAI,CAACC,MAAjB;MACD,CAHO,CAAR;IAID","names":["useStore","computed","isProxy","toRaw","nextTick","store","currentOrder","state","currencyOrder","changeStatus","item","status","console","log"],"sources":["C:/Users/user/Desktop/admin/coreui-free-vue-admin-template-main/coreui-free-vue-admin-template-main/src/views/CurrencyOrder.vue"],"sourcesContent":["<template>\n  <div>\n    <CForm>\n      <CTable align=\"middle\" class=\"mb-0 border\" hover responsive>\n        <CTableHead color=\"light\">\n     