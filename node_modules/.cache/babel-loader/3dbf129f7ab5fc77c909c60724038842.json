{"ast":null,"code":"import { useStore } from 'vuex';\nimport { ref, computed, isProxy, nextTick } from 'vue';\nimport { api } from '../apiRequset.js';\nexport default {\n  __name: 'CurrentOrder',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const store = useStore();\n    const currentOrder = ref();\n    getCurrentOrder();\n    computed(() => {\n      if (isProxy(store.state.updateCurrentOrderTime)) {\n        //this If() block is not really necessary\n        getCurrentOrder();\n      }\n\n      return null;\n    });\n\n    function getCurrentOrder() {\n      api.getCurrentOrder().then(response => {\n        currentOrder.value = response.data;\n      });\n    }\n\n    function changeStatus(item) {\n      api.changeOrderStatus({\n        orderId: item.orderId,\n        status: 3\n      }).then(response => {\n        console.log(response);\n      });\n      item.status = item.status == 0 ? 1 : 0;\n      setInterval(getCurrentOrder, 5000);\n    }\n\n    const __returned__ = {\n      store,\n      currentOrder,\n      getCurrentOrder,\n      changeStatus,\n      useStore,\n      ref,\n      computed,\n      isProxy,\n      nextTick,\n      api\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AA6DA,SAASA,QAAT,QAAyB,MAAzB;AACA,SAASC,GAAT,EAAcC,QAAd,EAAwBC,OAAxB,EAAiCC,QAAjC,QAAiD,KAAjD;AACA,SAASC,GAAT,QAAoB,kBAApB;;;;;;;;IACA,MAAMC,KAAK,GAAGN,QAAQ,EAAtB;IACA,MAAMO,YAAY,GAAGN,GAAG,EAAxB;IACAO,eAAe;IACfN,QAAQ,CAAC,MAAM;MACb,IAAIC,OAAO,CAACG,KAAK,CAACG,KAAN,CAAYC,sBAAb,CAAX,EAAiD;QAC/C;QACAF,eAAe;MAChB;;MACD,OAAO,IAAP;IACD,CANO,CAAR;;IAOA,SAASA,eAAT,GAA2B;MACzBH,GAAG,CAACG,eAAJ,GAAsBG,IAAtB,CAA4BC,QAAD,IAAc;QACvCL,YAAY,CAACM,KAAb,GAAqBD,QAAQ,CAACE,IAA9B;MACD,CAFD;IAGD;;IACD,SAASC,YAAT,CAAsBC,IAAtB,EAA4B;MAC1BX,GAAG,CACAY,iBADH,CACqB;QAAEC,OAAO,EAAEF,IAAI,CAACE,OAAhB;QAAyBC,MAAM,EAAE;MAAjC,CADrB,EAEGR,IAFH,CAESC,QAAD,