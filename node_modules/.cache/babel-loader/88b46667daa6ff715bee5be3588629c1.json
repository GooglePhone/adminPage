ntOrder.value\"\n            :key=\"item.name\"\n          >\n            <CTableDataCell class=\"text-center\">\n              {{ item.id }}\n            </CTableDataCell>\n            <CTableDataCell class=\"text-center\">\n              <div>{{ item.name }}</div>\n            </CTableDataCell>\n            <CTableDataCell class=\"text-center\">{{ item.num }}</CTableDataCell>\n            <CTableDataCell class=\"text-center\">\n              <div class=\"clearfix\">\n                <div>\n                  {{ item.price }}\n                </div>\n              </div>\n            </CTableDataCell>\n            <CTableDataCell class=\"text-center\">\n              <!-- {{ item.ctime }} -->\n            </CTableDataCell>\n            <CTableDataCell>\n              <div>\n                <CFormSwitch\n                  :label=\"item.status == 0 ? '未出餐' : '已出餐'\"\n                  id=\"formSwitchCheckChecked\"\n                  :check=\"item.status == 0\"\n                  @change=\"\n                    item.status == 1 ? (item.status = 0) : (item.status = 1)\n                  \"\n                />\n              </div>\n            </CTableDataCell>\n          </CTableRow>\n          <CTableRow> </CTableRow>\n        </CTableBody> </CTable\n    ></CForm>\n  </div>\n</template>\n<script setup>\n// import { reactive, onMounted } from 'vue'\n// import { websocket } from '../websocket.js'\nimport { wsStore } from '../wsStore'\n// // const tableExample = reactive([\n// //   {\n// //     dish: {\n// //       id: 185,\n// //       name: '肝臟',\n// //       price: 288,\n// //       num: 1,\n// //       status: 0,\n// //       ctime: '2022-02-28 12:22:22',\n// //     },\n// //   },\n// // ])\n// onMounted(() => {\n//   console.log(wsStore.currentOrder)\n//   console.log(websocket.data())\n// })\n</script>\n"]},"metadata":{},"sourceType":"module"}