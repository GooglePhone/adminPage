{"ast":null,"code":"import { useStore } from 'vuex';\nimport { ref, computed, isProxy } from 'vue';\nimport { api } from '../apiRequset.js';\nexport default {\n  __name: 'CurrentOrder',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const store = useStore();\n    const currentOrder = ref();\n    const currentOrderTime = ref();\n    getCurrentOrder();\n    currentOrderTime.value = computed(() => {\n      if (isProxy(store.state.updateCurrentOrderTime)) {\n        //this If() block is not really necessary\n        getCurrentOrder();\n        return store.state.updateCurrentOrderTime;\n      }\n\n      return null;\n    });\n\n    function getCurrentOrder() {\n      api.getCurrentOrder().then(response => {\n        currentOrder.value = response.data;\n      });\n    }\n\n    function changeStatus(item) {\n      api.changeOrderStatus({\n        orderId: item.id,\n        status: 3\n      }).then(response => {\n        console.log(response);\n      });\n      item.status = item.status == 0 ? 1 : 0;\n      setTimeout(getCurrentOrder, 50000);\n    }\n\n    const __returned__ = {\n      store,\n      currentOrder,\n      currentOrderTime,\n      getCurrentOrder,\n      changeStatus,\n      useStore,\n      ref,\n      computed,\n      isProxy,\n      api\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AA6DA,SAASA,QAAT,QAAyB,MAAzB;AACA,SAASC,GAAT,EAAcC,QAAd,EAAwBC,OAAxB,QAAuC,KAAvC;AACA,SAASC,GAAT,QAAoB,kBAApB;;;;;;;;IACA,MAAMC,KAAK,GAAGL,QAAQ,EAAtB;IACA,MAAMM,YAAY,GAAGL,GAAG,EAAxB;IACA,MAAMM,gBAAgB,GAAGN,GAAG,EAA5B;IACAO,eAAe;IACfD,gBAAgB,CAACE,KAAjB,GAAyBP,QAAQ,CAAC,MAAM;MACtC,IAAIC,OAAO,CAACE,KAAK,CAACK,KAAN,CAAYC,sBAAb,CAAX,EAAiD;QAC/C;QACAH,eAAe;QACf,OAAOH,KAAK,CAACK,KAAN,CAAYC,sBAAnB;MACD;;MACD,OAAO,IAAP;IACD,CAPgC,CAAjC;;IAQA,SAASH,eAAT,GAA2B;MACzBJ,GAAG,CAACI,eAAJ,GAAsBI,IAAtB,CAA4BC,QAAD,IAAc;QACvCP,YAAY,CAAC