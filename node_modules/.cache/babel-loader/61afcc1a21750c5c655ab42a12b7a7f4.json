{"ast":null,"code":"import { useStore } from 'vuex';\nimport { ref, watch } from 'vue';\nimport { api } from '../apiRequset.js';\nexport default {\n  __name: 'CurrentOrder',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const store = useStore();\n    const currentOrder = ref();\n    getCurrentOrder();\n    watch(() => store.state.updateCurrentOrderTime, function () {\n      getCurrentOrder();\n    });\n\n    function getCurrentOrder() {\n      api.getCurrentOrder().then(response => {\n        currentOrder.value = response.data;\n      });\n    }\n\n    function changeStatus(item) {\n      api.changeOrderStatus({\n        orderId: item.id,\n        status: item.status == 1 ? 2 : 1\n      }).then(response => {\n        console.log(response);\n      });\n      item.status = item.status == 0 ? 1 : 0;\n      setTimeout(getCurrentOrder, 50000);\n    }\n\n    const __returned__ = {\n      store,\n      currentOrder,\n      getCurrentOrder,\n      changeStatus,\n      useStore,\n      ref,\n      watch,\n      api\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AA8DA,SAASA,QAAT,QAAyB,MAAzB;AACA,SAASC,GAAT,EAAcC,KAAd,QAA2B,KAA3B;AACA,SAASC,GAAT,QAAoB,kBAApB;;;;;;;;IACA,MAAMC,KAAK,GAAGJ,QAAQ,EAAtB;IACA,MAAMK,YAAY,GAAGJ,GAAG,EAAxB;IACAK,eAAe;IAEfJ,KAAK,CACH,MAAME,KAAK,CAACG,KAAN,CAAYC,sBADf,EAEH,YAAY;MACVF,eAAe;IAChB,CAJE,CAAL;;IAOA,SAASA,eAAT,GAA2B;MACzBH,GAAG,CAACG,eAAJ,GAAsBG,IAAtB,CAA4BC,QAAD,IAAc;QACvCL,YAAY,CAACM,KAAb,GAAqBD,QAAQ,CAACE,IAA9B;MACD,CAFD;IAGD;;IACD,SAASC,YAAT,CAAsBC,IAAtB,EAA4B;MAC1BX,GAAG,CACAY,iBADH,CACqB;QAAEC,OAAO,EAAEF,IAAI,CAACG,EAAhB;QAAoBC,MAAM,EAAEJ,IAAI,CAACI,MAAL,IAAe,CAAf,GAAmB,CAAnB,GAAuB;MAAnD,CADrB,EAEGT,IAFH,CAESC,QAAD,IAAc;QAClBS,OAAO,CAACC,GAAR,CAAYV,QAAZ;MACD,CAJH;MAKAI,IAAI,CAACI,MAAL,GAAcJ,IAAI,CAACI,MAAL,IAAe,CAAf,GAAmB,CAAnB,GAAuB,CAArC;MACAG,UAAU,CAACf,eAAD,EAAkB,KAAlB,CAAV;I